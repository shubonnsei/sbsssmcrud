<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jp.co.sony.ppog.mapper.CountryMapper">
	<resultMap id="BasicResultMap"
		type="jp.co.sony.ppog.entity.Country" autoMapping="true">
		<id property="code" jdbcType="NVARCHAR" column="code" />
	</resultMap>
	<select id="findNationCode" parameterType="java.lang.String"
		resultType="java.lang.String">
		select cty.code from country as cty where cty.name = #{name;jdbcType=NVARCHAR}
	</select>
</mapper>